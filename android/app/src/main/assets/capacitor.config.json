{
  "appId": "international.idems.plh_teens",
  "appName": "Parenting for Lifelong Health",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000
    },
    "CapacitorFirebaseAuth": {
      "providers": ["google.com", "twitter.com", "facebook.com", "phone"],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": ["profile"]
      }
    }
  },
  "allowNavigation": ["example.org", "*.example.org", "192.0.2.1"]
}
