{
  "appId": "international.idems.plh_teens",
  "appName": "PLH Teens",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000
    },
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile"
        ]
      }
    }
  },
  "allowNavigation": [],
  "server": {
    "url": "http://192.168.1.20:8100",
    "cleartext": true
  }
}
