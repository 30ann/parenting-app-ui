{
  "flow_type": "template",
  "flow_name": "home_screen_modular",
  "status": "released",
  "override_target": "home_screen",
  "override_condition": "@fields._app_skin == \"modular\"",
  "rows": [
    {
      "name": "highlighted_module",
      "value": "@fields._highlighted_task_group",
      "_translations": {
        "value": {}
      },
      "condition": false,
      "type": "set_variable",
      "_nested_name": "highlighted_module",
      "_dynamicFields": {
        "value": [
          {
            "fullExpression": "@fields._highlighted_task_group",
            "matchedExpression": "@fields._highlighted_task_group",
            "type": "fields",
            "fieldName": "_highlighted_task_group"
          }
        ]
      },
      "_dynamicDependencies": {
        "@fields._highlighted_task_group": [
          "value"
        ]
      }
    },
    {
      "type": "template",
      "name": "highlighted_module",
      "value": "module_card",
      "rows": [
        {
          "name": "workshop_id",
          "value": "@local.highlighted_module",
          "_translations": {
            "value": {}
          },
          "condition": false,
          "type": "set_variable",
          "_nested_name": "highlighted_module.workshop_id",
          "_dynamicFields": {
            "value": [
              {
                "fullExpression": "@local.highlighted_module",
                "matchedExpression": "@local.highlighted_module",
                "type": "local",
                "fieldName": "highlighted_module"
              }
            ]
          },
          "_dynamicDependencies": {
            "@local.highlighted_module": [
              "value"
            ]
          }
        },
        {
          "name": "card_style",
          "value": "landscape",
          "_translations": {
            "value": {}
          },
          "type": "set_variable",
          "_nested_name": "highlighted_module.card_style"
        }
      ],
      "_nested_name": "highlighted_module"
    },
    {
      "type": "display_group",
      "name": "dg_uncompleted",
      "parameter_list": {
        "style": "column"
      },
      "rows": [
        {
          "type": "title",
          "name": "title_uncompleted",
          "value": "Upcoming modules",
          "_translations": {
            "value": {}
          },
          "_nested_name": "dg_uncompleted.title_uncompleted"
        },
        {
          "type": "carousel",
          "name": "carousel_uncompleted",
          "parameter_list": {
            "space_between": "8"
          },
          "rows": [
            {
              "type": "items",
              "value": "@data.workshop_tasks",
              "parameter_list": {
                "filter": "!@item.completed"
              },
              "rows": [
                {
                  "type": "template",
                  "name": "uncompleted_modules",
                  "value": "module_card",
                  "rows": [
                    {
                      "name": "workshop_id",
                      "value": "@item.id",
                      "_translations": {
                        "value": {}
                      },
                      "type": "set_variable",
                      "_nested_name": "dg_uncompleted.carousel_uncompleted.items.uncompleted_modules.workshop_id",
                      "_dynamicFields": {
                        "value": [
                          {
                            "fullExpression": "@item.id",
                            "matchedExpression": "@item.id",
                            "type": "item",
                            "fieldName": "id"
                          }
                        ]
                      },
                      "_dynamicDependencies": {
                        "@item.id": [
                          "value"
                        ]
                      }
                    }
                  ],
                  "_nested_name": "dg_uncompleted.carousel_uncompleted.items.uncompleted_modules"
                }
              ],
              "name": "items",
              "_nested_name": "dg_uncompleted.carousel_uncompleted.items",
              "_dynamicFields": {
                "value": [
                  {
                    "fullExpression": "@data.workshop_tasks",
                    "matchedExpression": "@data.workshop_tasks",
                    "type": "data",
                    "fieldName": "workshop_tasks"
                  }
                ],
                "parameter_list": {
                  "filter": [
                    {
                      "fullExpression": "!@item.completed",
                      "matchedExpression": "!@item.completed",
                      "type": "item",
                      "fieldName": "completed"
                    }
                  ]
                }
              },
              "_dynamicDependencies": {
                "@data.workshop_tasks": [
                  "value"
                ],
                "!@item.completed": [
                  "parameter_list.filter"
                ]
              }
            }
          ],
          "_nested_name": "dg_uncompleted.carousel_uncompleted"
        }
      ],
      "_nested_name": "dg_uncompleted"
    },
    {
      "type": "display_group",
      "name": "dg_completed",
      "parameter_list": {
        "style": "column"
      },
      "rows": [
        {
          "type": "title",
          "name": "title_completed",
          "value": "Completed modules",
          "_translations": {
            "value": {}
          },
          "_nested_name": "dg_completed.title_completed"
        },
        {
          "type": "carousel",
          "name": "carousel_completed",
          "parameter_list": {
            "space_between": "8"
          },
          "rows": [
            {
              "type": "items",
              "value": "@data.workshop_tasks",
              "parameter_list": {
                "filter": "@item.completed"
              },
              "rows": [
                {
                  "type": "template",
                  "name": "completed_modules",
                  "value": "module_card",
                  "rows": [
                    {
                      "name": "workshop_id",
                      "value": "@item.id",
                      "_translations": {
                        "value": {}
                      },
                      "type": "set_variable",
                      "_nested_name": "dg_completed.carousel_completed.items.completed_modules.workshop_id",
                      "_dynamicFields": {
                        "value": [
                          {
                            "fullExpression": "@item.id",
                            "matchedExpression": "@item.id",
                            "type": "item",
                            "fieldName": "id"
                          }
                        ]
                      },
                      "_dynamicDependencies": {
                        "@item.id": [
                          "value"
                        ]
                      }
                    }
                  ],
                  "_nested_name": "dg_completed.carousel_completed.items.completed_modules"
                }
              ],
              "name": "items",
              "_nested_name": "dg_completed.carousel_completed.items",
              "_dynamicFields": {
                "value": [
                  {
                    "fullExpression": "@data.workshop_tasks",
                    "matchedExpression": "@data.workshop_tasks",
                    "type": "data",
                    "fieldName": "workshop_tasks"
                  }
                ],
                "parameter_list": {
                  "filter": [
                    {
                      "fullExpression": "@item.completed",
                      "matchedExpression": "@item.completed",
                      "type": "item",
                      "fieldName": "completed"
                    }
                  ]
                }
              },
              "_dynamicDependencies": {
                "@data.workshop_tasks": [
                  "value"
                ],
                "@item.completed": [
                  "parameter_list.filter"
                ]
              }
            }
          ],
          "_nested_name": "dg_completed.carousel_completed"
        }
      ],
      "_nested_name": "dg_completed"
    },
    {
      "type": "drawer",
      "value": "Things to do",
      "rows": [
        {
          "type": "display_group",
          "parameter_list": {
            "style": "column"
          },
          "rows": [
            {
              "type": "button",
              "value": "Why self care",
              "_translations": {
                "value": {}
              },
              "parameter_list": {
                "style": "card",
                "icon": "plh_images/workshops/w_self_care/tools.svg"
              },
              "name": "button",
              "_nested_name": "drawer.display_group.button"
            },
            {
              "type": "button",
              "value": "Relax",
              "parameter_list": {
                "style": "card",
                "icon": "plh_images/icons/parent_child.svg"
              },
              "rows": [
                {
                  "type": "toggle_bar",
                  "name": "toggle_bar",
                  "parameter_list": {
                    "position": "right",
                    "style": "in_button"
                  },
                  "_nested_name": "drawer.display_group.button.toggle_bar"
                }
              ],
              "name": "button",
              "_nested_name": "drawer.display_group.button"
            },
            {
              "type": "button",
              "value": "Something fun",
              "_translations": {
                "value": {}
              },
              "parameter_list": {
                "style": "card",
                "icon": "plh_images/icons/book.svg"
              },
              "name": "button",
              "_nested_name": "drawer.display_group.button"
            }
          ],
          "name": "display_group",
          "_nested_name": "drawer.display_group"
        }
      ],
      "name": "drawer",
      "_nested_name": "drawer"
    }
  ],
  "_xlsxPath": "global_modular/core_templates/home_screen_modular.xlsx"
}